<Window x:Class="FlightBoardX.JobRuning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        Title="JobRuning" Height="350" Width="340" Loaded="JobRuning_OnLoaded"  WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="FinnishButton" Content="Finnish" HorizontalAlignment="Left" Margin="51,0,0,10" VerticalAlignment="Bottom" Width="75" IsDefault="True" Click="FinnishButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="178,0,0,10" VerticalAlignment="Bottom" Width="75" Click="CancelButton_Click"/>
        <Image gif:ImageBehavior.AnimatedSource="Flying01.gif" Height="200" Margin="5,5,0,0" VerticalAlignment="Top"  />
        <Label x:Name="TimeLabel" Content="Timer" HorizontalAlignment="Left" Margin="50,255,0,0" VerticalAlignment="Top"/>
        <Label x:Name="StartLabel" Content="Start" HorizontalAlignment="Left" Margin="10,215,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
